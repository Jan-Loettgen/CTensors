
CC=gcc
CFLAGS=-I -g -Wall -Werror
OBJS=obj/main.o obj/tensor_2d.o obj/tensor_2d_threaded.o
DEPS=src/tensor_2d.h src/tensor_2d_threaded.h
BIN=bin/main

all: $(BIN)

bin/main: $(OBJS)
	$(CC) $(CFLAGS) $(OBJS) -o main

obj/%.o: src/%.c src/%.h
	$(CC) $(CFLAGS) -c -o $(OBJS)

clean:
	$(RM) -r bin/* obj/*
